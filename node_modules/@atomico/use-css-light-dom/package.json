{
	"name": "@atomico/use-css-light-dom",
	"description": "Series of utilities in hooks format to extend the operation of Atomico",
	"version": "1.0.1",
	"type": "module",
	"license": "MIT",
	"author": {
		"name": "UpperCod",
		"email": "uppercod.git@gmail.com",
		"url": "https://github.com/uppercod"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/atomicojs/hooks.git"
	},
	"bugs": {
		"url": "https://github.com/atomicojs/hooks/issues"
	},
	"homepage": "https://atomico.gitbook.io/doc/atomico/atomico-hooks",
	"scripts": {
		"build:compile": "tsc",
		"build:exports": "exports \"dist/**/*\"",
		"test": "vitest run"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"atomico": "^1.75.1"
	},
	"exports": {
		"./use-css-light-dom": {
			"default": "./dist/use-css-light-dom.js",
			"types": "./dist/use-css-light-dom.d.ts"
		},
		"./utils": {
			"types": "./dist/utils.d.ts",
			"default": "./dist/utils.js"
		},
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"use-css-light-dom": [
				"dist/use-css-light-dom.d.ts"
			],
			"utils": [
				"dist/utils.d.ts"
			]
		}
	},
	"types": "dist/index.d.ts"
}
